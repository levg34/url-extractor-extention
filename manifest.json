{
	"manifest_version": 2,

	"name": "URL Extractor Plugin",
	"description": "This extension will ease your navigation using URL Extractor",
	"version": "1.0",
	
	"options_page": "options.html",
	
	"icons": {
		"16": "icon16.png",
        "48": "icon48.png",
		"128": "icon128.png"
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Click here!"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"storage",
		"*://*/*"
	]
}
